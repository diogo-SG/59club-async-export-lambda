{
  "name": "59club-async-export-lambda",
  "version": "1.0.0",
  "description": "AWS Lambda function for handling PDF report generation with Puppeteer",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:local": "node test/local-server.js",
    "test": "node test/local-test.js",
    "test:backend": "node test/test-with-backend.js",
    "test:check": "node test/test-with-backend.js check",
    "build": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true npm install --production",
    "package": "node scripts/package-lambda.js",
    "deploy": "aws lambda update-function-code --function-name 59club-async-export-lambda --zip-file fileb://function.zip",
    "deploy:quick": "./quick-deploy.sh",
    "deploy:dev": "./quick-deploy.sh 59club-async-export-dev us-east-1 dev",
    "deploy:staging": "./quick-deploy.sh 59club-async-export-staging us-east-1 staging",
    "deploy:prod": "./quick-deploy.sh 59club-async-export-prod us-east-1 prod"
  },
  "dependencies": {
    "puppeteer-core": "21.3.0",
    "@sparticuz/chromium": "131.0.1",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1691.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "aws-lambda",
    "puppeteer",
    "pdf-generation",
    "serverless",
    "report-export"
  ],
  "author": "Studio Graphene",
  "license": "MIT"
}
